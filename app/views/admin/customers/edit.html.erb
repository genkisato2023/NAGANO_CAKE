<h2>顧客情報編集</h2>
<%= form_with model: @customer do |f| %>
　<label for="inputLast_name">姓</label><br>
  <%= f.text_field :last_name, autofocus: true, id:"inputLast_name", placeholder:"姓" %><br>
  
  <label for="inputFirst_name">名</label><br>
  <%= f.text_field :first_name, autofocus: true, id:"inputFirst_name", placeholder:"名" %><br>
  
  <label for="inputLast_name_kana">姓カナ</label><br>
  <%= f.text_field :last_name_kana, autofocus: true, id:"inputLast_name_kana", placeholder:"姓カナ" %><br>
  
  <label for="inputFirst_name_kana">名カナ</label><br>
  <%= f.text_field :first_name_kana, autofocus: true, id:"inputFirst_name_kana", placeholder:"名カナ" %><br>
  
  <label for="inputEmail">メールアドレス</label><br>
  <%= f.text_field :email, autofocus: true, id:"inputEmail", placeholder:"メールアドレス" %><br>
  
  <label for="inputEncrypted_password">パスワード</label><br>
  <%= f.text_field :encrypted_password, autofocus: true, id:"inputEncrypted_password", placeholder:"パスワード" %><br>
  
  <label for="inputPostal_code">郵便番号</label><br>
  <%= f.text_field :postal_code, autofocus: true, id:"inputPostal_code", placeholder:"郵便番号"%><br>
  
  <label for="inputAddress">住所</label><br>
  <%= f.text_field :address, autofocus: true, id:"inputAddress", placeholder:"住所"%><br>
  
  <label for="inputTelephone_number">電話番号</label><br>
  <%= f.text_field :telephone_number, autofocus: true, id:"inputTelephone_number", placeholder:"電話番号"%><br>
  
  <label for="inputIs_deleted">退会ステータス</label><br>
  <%= f.text_field :is_deleted, autofocus: true, id:"inputIs_deleted", placeholder:"退会ステータス"%><br>
  
  <%= f.submit "変更を保存" %>
<% end %>

<p><%= link_to "戻る", admin_customer_path(customer.id) %></p>　|
<p><%= link_to "顧客一覧へ", admin_customers_path %></p>


